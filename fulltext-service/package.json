{
  "name": "fulltext-service",
  "version": "1.0.0",
  "description": "Fulltext search service for find it",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm i -g nodemon node-inspector rimraf npm-run-all npm-check",
    "start": "npm-run-all -s clean build start:simple",
    "start:simple": "node .",
    "start:dev": "npm-run-all -s clean build -p watch:*",
    "start:debug": "npm-run-all -s clean build -p watch:* debug",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "debug": "node-debug",
    "build": "npm-run-all -s build:ts",
    "build:ts": "./node_modules/typescript/bin/tsc",
    "watch:ts": "./node_modules/typescript/bin/tsc --watch",
    "watch:node": "nodemon --debug --delay 2.5",
    "lint": "tslint ./src/{,**/}*.ts"
  },
  "engines": {
    "node": "~6.3"
  },
  "engineStrict": true,
  "author": "neXr",
  "license": "MIT",
  "dependencies": {
    "@types/elasticsearch": "^5.0.17",
    "body-parser": "^1.15.2",
    "config": "^1.21.0",
    "cors": "^2.7.1",
    "elasticsearch": "^14.0.0",
    "event-dispatch": "^0.4.1",
    "express": "^4.14.0",
    "express-ping": "^1.1.0",
    "express-winston": "^1.4.2",
    "glob": "^7.0.5",
    "js-yaml": "^3.6.1",
    "reflect-metadata": "^0.1.3",
    "routing-controllers": "^0.7.6",
    "typedi": "^0.5.2",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.29",
    "@types/chai": "^3.4.26-alpha",
    "@types/config": "0.0.29",
    "@types/cors": "0.0.29",
    "@types/express": "^4.0.29",
    "@types/glob": "^5.0.29",
    "@types/js-yaml": "^3.5.25-alpha",
    "@types/mocha": "^2.2.25-alpha",
    "@types/node": "^4.0.26-alpha",
    "@types/serve-static": "^1.7.27",
    "@types/supertest": "^2.0.3",
    "@types/winston": "^2.2.25-alpha",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.3.3",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "remap-istanbul": "^0.6.4",
    "source-map-support": "^0.4.1",
    "supertest": "^3.0.0",
    "ts-helpers": "^1.1.1",
    "tslint": "^5.8.0",
    "typescript": "^2.0.0",
    "npm-run-all": "^3.0.0",
    "rimraf": "^2.0.0"
  }
}
